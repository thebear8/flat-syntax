{
  "name": "flat-syntax",
  "displayName": "flat-syntax",
  "description": "Syntax highlighting for flat",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "flat",
      "aliases": ["Flat", "flat"],
      "extensions": [".fl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "flat",
      "scopeName": "source.flat",
      "path": "./syntaxes/flat.tmLanguage.json"
    }]
  }
}
